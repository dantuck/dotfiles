[include]
  path = config.local 
[user]
  useConfigOnly = true
[fetch]
  prune = true
  pruneTags = true	
[alias]
	pt = !git tag -l | xargs git tag -d && git fetch -t
	sort = !git tag -l | sort -V
[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
[url "git@github.com:"]
	insteadOf = https://github.com/
[url "git@gitlab.com:"]
	insteadOf = https://gitlab.com/
